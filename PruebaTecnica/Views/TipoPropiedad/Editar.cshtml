@model PruebaTecnica.Models.TipoPropiedad
@{
    ViewData["Title"] = "Editar tipo de propiedad";
}
<h1>@ViewData["Title"]</h1>



<form asp-controller="TipoPropiedad" asp-action="Edit" asp-route-id="@Model.Id">
    <div class="mb-3 mt-3">
        <label for="email" class="form-label">Tipo de propiedad:</label>
        <input type="text"  class="form-control" id="descripcion" name="descripcion" value="@Model.Descripcion">

       
    </div>

    <button type="submit" class="btn btn-success">Guardar</button>
</form>
<br />




